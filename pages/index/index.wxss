

.v-nav {
  margin: -50rpx 30rpx 30rpx;
  width: auto;
}

.my-list.menu>.my-item:after {
  border: none;
}

.v-modal .my-dialog {
  padding: 100rpx 80rpx 0;
}

.user-container {
  height: 208rpx;
}
.userinfo {
  display: flex;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  overflow: hidden;
}
.slide {
  display: inline-block;
  width: 44rpx;
  height: 33rpx;
}
.banner {
  display: block;
  width: 100%;
  height: 160rpx;
}
.add {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
}
.my-bar.fixed, .nav.fixed {
  box-shadow: none;
}
.power {
  display:block;
  width: 28rpx;
  height: 38rpx;
}
.shadow-box {
  box-shadow: 0 0 14rpx 12rpx var(--blueShadow);
}
.weui-search-bar {
   height: 36px;
   line-height: 36px;
    position: relative;
    padding: 5px 15px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-sizing: border-box;
    /* background-color: #EFEFF4; */
    /* border-top: 1rpx solid #D7D6DC; */
    /* border-bottom: 1rpx solid #D7D6DC; */
   
 }
 .weui-icon-search {
  margin-right: 7px;
  font-size: inherit;
  position: absolute;
    left: 180rpx;
    top: 4px;

 }
 .weui-icon-search_in-box {
  position: absolute;
  left: 15px;
  top: 7px;
 }
 .weui-search-bar__text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
 }
 .weui-search-bar__form {
  height: 36px;
  line-height: 36px;
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
   flex: auto;
  border-radius: 30px;
  background: #FFFFFF;
  /* border: 1px solid #E6E6EA; */
 }
 .weui-search-bar__box {
  height: 36px;
  line-height: 36px;
  position: relative;
  padding-left: 40px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
 }
 .weui-search-bar__input {
   /* margin-top: 10px; */
  height: 36px;
   line-height: 36px;
  font-size: 14px;
 }
 .weui-icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 12px;
  font-size: 0;
 }
 .weui-search-bar__label {
  height: 36px;
  line-height: 36px;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9B9B9B;
  background: #FFFFFF;
 
 }
 .weui-search-bar__cancel-btn {
  margin-left: 10px;
   line-height: 36px;
  color: #09BB07;
  white-space: nowrap;
  font-size: 30rpx;
 }