
.v-nav {
  margin: -50rpx 30rpx 30rpx;
  width: auto;
}

.my-list.menu>.my-item:after {
  border: none;
}

.v-modal .my-dialog {
  padding: 100rpx 80rpx 0;
}

.user-container {
  height: 208rpx;
}
.userinfo {
  display: flex;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  overflow: hidden;
}
.slide {
  display: inline-block;
  width: 44rpx;
  height: 33rpx;
}
.banner {
  display: block;
  width: 100%;
  height: 160rpx;
}
.add {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
}
.my-bar.fixed, .nav.fixed {
  box-shadow: none;
}
.power {
  display:block;
  width: 28rpx;
  height: 38rpx;
}
.shadow-box {
  box-shadow: 0 0 14rpx 12rpx var(--blueShadow);
}