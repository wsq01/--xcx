
<custom-bar bgImage="data:image/png;base64," isHome="{{true}}">
	<view slot="homeText" class="flex align-center">
		<image class="slide" mode="aspectfill" bindtap="showModal" data-target="drawerModal" src="/images/slide@2x.png"></image>
		<view class="flex align-center">
			<text class='margin-left-sm'>添加设备</text>
		</view>
	</view>
</custom-bar>
<view>
	<image class="banner" mode="aspectfill" src="/images/banner@2x.png"></image>
	<scroll-view scroll-x class="bg-white nav v-nav radius">
		<view class="flex text-center">
			<picker mode = "selector" bindchange="bindPickerChange" value="{{project[idx].id}}" range="{{project}}" range-key="{{'val'}}">
          <view class="picker picker1 flex align-center">
           <text class="my-item flex-sub">{{project[idx].val}}</text> 	<image class="xiala " mode="aspectfill" src="/images/sanjiao.png"></image>
          </view>
      </picker>
      <view class="flex align-center">
        <input class="weui-input searchbox"  bindinput="inputTyping" placeholder="按设备名称、设备号搜索" />
        <image src="/images/search-img.png" class="searchimg" bindtap="querydev"></image>
      </view>
		</view>
	</scroll-view>

	<swiper class='swiper flex' bindchange='swiperChange' current="{{TabCur}}" style="height: calc(100vh - 350rpx)">
 
		<swiper-item class="tab-swiper-0  flex">
        <scroll-view scroll-y="true" class="swiper-wrapper-left" scroll-x='true'>
        <view class="swiper-wrapper">
          <view wx:for="{{scrolls}}" wx:key="index" class="swiper-item text-center">
            <view class="text-center {{item.number  == activeval ? 'swiper-item-active' : ''}}" data-id="{{item.number}}"  bindtap="handleJumpPage">{{item.gongsimingcheng}}</view>
          </view>
        </view>
      </scroll-view>
      <scroll-view refresher-enabled="{{true}}" refresher-triggered="{{isTriggered}}"  scroll-x='true' bindrefresherrefresh="bindrefresherrefresh" scroll-y bindscrolltolower="bindscrolltolower"  refresher-threshold="{{100}}" refresher-default-style="black" refresher-background="#f0f0f0" style="height: calc(100vh - 350rpx);width: calc(100vw - 80px);">
        <view>
          <view class="text-center text-gray align-center flex " style="display:flex;justify-content: center;" wx:if="{{activeval==scrolls[0].number}}">当前绑定设备数:<text class="text-blue"> {{count1}} </text>台,待缴设备数:<navigator class="text-red" url="../bill/bill" hover-class="navigator-hover">{{daoqicount}}</navigator> 台</view>
        <view class="text-center text-gray  align-center flex" style="display:flex;justify-content: center;"  wx:else>当前账户设备数:<text class="text-blue"> {{count1}} </text>台 ,待缴设备数:<navigator class="text-red" url="../bill/bill" hover-class="navigator-hover">{{daoqicount}}</navigator> 台</view>
          <!-- style="height: calc(100vh - 350rpx);" -->
        <view class="radius-lg shadow-box marginB animation-{{animation1 ? 'slide-bottom' : ''}}" wx:for="{{devList}}" wx:key="index" bindtap="toPage" data-type="redirect" data-page="../device/detail/detail?devid={{item.shebeibianhao}}&isMaster={{item.is_master}}&model_type={{item.model_type}}&address={{item.address}}" >
          <view class="radius-lg bg-white padding-tb-sm padding-lr item-black">

            <view class="flex solids-bottom">
              <view class="flex-sub flex justify-between padding-tb-xs align-center">
                <view catchtap="bindShowSetNameModal" data-id="{{item.shebeibianhao}}" data-name="{{item.beizhu}}" class="flex-sub padding-right text-cut {{item.beizhu ? '' : 'text-gray'}}">{{item.beizhu ? item.beizhu : '点击设置昵称'}}</view>
                <view>
                  {{item.shebeibianhao}}
                 
                </view>
              </view>
              <view class="solids-right margin-lr-sm margin-tb-sm"></view>
              <view class="padding-tb-sm {{item.is_line ? 'text-green' : 'text-gray'}}">{{item.is_line ? '在线' : '离线'}}</view>
            </view>
            <view class="flex justify-between padding-tb-sm">
              <view class="flex flex-direction align-center justify-center">
                <image style="display: block;width: 56rpx;height: 48rpx;" src="{{!item.is_line ? '/images/temp-off@2x.png' : ((item.baojingwendu_shangxian_baojing === 1 && item.last_temperature01 >= item.baojingwendu_shangxian) || (item.baojingwendu_xiaxian_baojing === 1 && item.baojingwendu_xiaxian >= item.item.last_temperature01)) ? '/images/temp-high@2x.png' : '/images/temp@2x.png'}}"></image>
                <text class="margin-top-xs">{{item.last_temperature01}}°C</text>
              </view>
              <view wx:if="{{item.model_type==='TT'}}" class="flex flex-direction align-center justify-center">
                <image style="display: block;width: 18rpx;height: 42rpx;" src="{{!item.is_line ? '/images/temp-off@2x.png' : ((item.baojingwendu_two_shangxian_baojing === 1 && item.last_temperature02 >= item.baojingwendu_two_shangxian) || (item.baojingwendu_two_xiaxian_baojing === 1 && item.last_temperature02 <= item.baojingwendu_two_xiaxian)) ? '/images/temp-heigh@2x.png' : '/images/temp@2x.png'}}"></image>
                <text class="margin-top-xs">{{item.last_temperature02}}°C</text>
              </view>
              <view wx:else class="flex flex-direction align-center justify-center">
                <image src="{{!item.is_line ? '/images/humidity-off@2x.png' : ((item.chaodishidubaojing === 1 && item.chaodishidubaojingfazhi >= item.last_humidity) || (item.chaogaoshidubaojing === 1 && item.chaogaoshidubaojingfazhi <= item.last_humidity)) ? '/images/humidity-heigh@2x.png' : '/images/humidity@2x.png'}}" style="display: block;width: 42rpx;height: 42rpx;"></image>
                <text class="margin-top-xs">{{item.last_humidity}}%RH</text>
              </view>
              <view class="flex flex-direction align-center justify-center">
                <image class="power" src="{{!item.is_line ? '/images/power-off@2x.png' : item.dianliang_xiaxian_baojing === 1 && item.last_power <=item.dianliang_xiaxian ? '/images/power1-low@2x.png' : item.last_power > 80 ? '/images/power4@2x.png' : item.last_power > 60 ? '/images/power3@2x.png' : item.last_power > 40 ? '/images/power2@2x.png' :item.last_power > 20 ? '/images/power2@2x.png': '/images/power1@2x.png'}}"></image>
                <text class="margin-top-xs">{{item.last_power}}%</text>
              </view>
              <view class="flex flex-direction align-center justify-center">
                <image style="display:block;width: 34rpx;height: 30rpx;" src="{{!item.is_line ? '/images/signal-off@2x.png' : item.xinhaoqiangdu >=26 ? '/images/signal5@2x.png' : item.xinhaoqiangdu >=20?'/images/signal4@2x.png': item.xinhaoqiangdu >=13?'/images/signal3@2x.png': item.xinhaoqiangdu >=5?'/images/signal2@2x.png':'/images/signal1@2x.png'}}"></image>
                <text class="margin-top-xs">{{item.xinhaoqiangdu >= 26 ? '强':item.xinhaoqiangdu >= 20 ? '好':item.xinhaoqiangdu >= 13 ? '良':item.xinhaoqiangdu >= 5 ? '弱':'无' }}</text>
              </view>
            </view>
						<view class="padding-top-sm solid-top">
              <view>
                <text class="myIcon-timefill text-blue margin-right-xs"></text>
                <text>采集时间：{{item.last_time}}</text>
              </view>
              <view class="flex">
                <text style="margin-top: 6rpx;" class="myIcon-locationfill text-blue margin-right-xs"></text>
                <text style="min-width: fit-content">地理位置：</text>
								<text class="{{idx==3?'myaddress':''}}">{{item.address}}</text>
              </view>
            </view>
            <view class="daoqi"  catchtap="bindDaoqi" wx:if="{{item.is_guoqi}}">
								<view class="h4">设备已到期</view>
								<view class="daoqicontent">
									您的设备服务已到期，请于<text class="daoqiTime">{{item.daoqishijian}}</text>前进行缴费，逾期该设备SIM卡将被注销。
								</view>
								<view>
									<button>前往缴费</button>
								</view>
							</view>
							
          </view>
        </view>
        <view wx:if="{{isShowLoadMore1}}" class="my-load margin-bottom-sm {{!isLoad1 ? 'loading':'over'}}"></view>
       </view>
      </scroll-view>
		</swiper-item>

	</swiper>

</view>

<view class=" v-modal my-modal drawer-modal justify-start {{modalName=='drawerModal'?'show':''}}" bindtap="hideModal" style="z-index:99999;">
	<view class="my-dialog basis-xl bg-white" catchtap style="height:calc(100vh)">

		<view class="user-container padding-bottom-lg">
			<view class="userinfo">
				<open-data class="userinfo-avatar" type="userAvatarUrl"></open-data>
				<open-data class="text-cut" type="userNickName"></open-data>
			</view>
		</view>

		<view class="my-list menu">
			<view class="my-item animation-{{animation ? 'slide-left':''}} margin-tb-sm" wx:for="{{menuList}}" bindtap="toPageWidthVerify" data-type="{{item.type}}" data-page="{{item.page}}" style="animation-delay: {{(index+1)*0.1}}s;" wx:key="index">
				<view class="content text-left flex align-center">
					<image src="{{item.icon}}"></image>
					<text class="">{{item.text}}</text>
				</view>
			</view>
		</view>

		<view class="flex justify-center" style="position: absolute;bottom: 40rpx;left: 80rpx;right: 80rpx;">
			<view class="padding-right-lg solids-right flex align-center" bindtap="toPage" data-page="../add/add" wx:if="{{isutypeb}}">
				<image class="add margin-right-sm" src="/images/icon-add@2x.png"></image>
				<text class="text-center">添加设备</text>
			</view>
			<navigator class="padding-left-lg flex align-center">
        <view class="flex align-center" bindtap="toEdit">
          <image class="add margin-right-sm" src="/images/icon-quit@2x.png"></image>
          <text>退出</text>
        </view>
			</navigator>
		</view>

	</view>
</view>

<view class="my-modal {{modalName=='setName'?'show':''}}">
	<view class="my-dialog">
		<view class="my-bar bg-white justify-end">
			<view class="content">设置昵称</view>
			<view class="action" bindtap="hideModal">
				<text class="myIcon-close text-red"></text>
			</view>
		</view>
		<view class="padding-xl">
			<form>
				<view class="my-form-group">
					<view class="title solids-right">昵称：</view>
					<input type="text" placeholder="请输入昵称" value="{{currDevice.name}}" bindinput="bindInputName"></input>
				</view>
			</form>
		</view>
		<view class="my-bar bg-white justify-end">
			<view class="action">
				<button class="my-btn line-blue text-blue" bindtap="hideModal">取消</button>
				<button class="my-btn bg-blue margin-left" bindtap="bindChangeName">确定</button>
			</view>
		</view>
	</view>
</view>

