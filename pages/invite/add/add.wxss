.fixedbar{
  position: fixed;
    top: 0;
    width: 100%;
    z-index: 99;
}
.choosedevice{
  margin-top: 304rpx;
  height: auto;
  border-top: 1px solid #eee;
}
.checkbox-group-content{
  width: 100%;
  height: 760rpx;
  overflow: scroll;
}
.checkbox-group-content .deviceline{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 84rpx;
  line-height: 84rpx;
  padding-right: 4rpx;
}
.checkbox-group-content .deviceline .num{
 display: inline-block;
 width: 110rpx;
 height: auto;
 margin-right: 10rpx;
}
.checkbox-group-content .deviceline .remarks{
  display: inline-block;
  width: auto;
  height: auto;
 }
 .my-form-group .listleft{
  display: flex;
  height: 100rpx;
  align-items: center;
 }
 .my-form-group .searchbox{
  display: flex;
  width: 380rpx;
  height: 50rpx;
  align-items: center;
  margin-left: 40rpx;
  position: relative;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
 }
 .my-form-group .searchbox .searchbtn{
   position: absolute;
   right: 10rpx;
   display: flex;
   z-index: 999;
 }
 .searchbox .searchbtn image{
   width: 28rpx;
   height: 28rpx;
 }
 .searchbox  input {
  flex: 1;
  font-size: 26rpx;
  color: #555;
  padding-right: 20rpx;
  padding-left: 20rpx;
}
.searchbtn text{
  margin:0 10rpx;
  font-size: 24rpx;
}
.checkbox-group-content .hidden{
  display: none;
}